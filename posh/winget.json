{
    "$schema": "https://aka.ms/winget-settings.schema.json",

    // For documentation on these settings, see: https://aka.ms/winget-settings
    "source": {
        "autoUpdateIntervalInMinutes": 5
    },
    "visual": {
        "progressBar": "accent"
    },
    "installBehavior": {
        "preferences": {
            "scope": "user",
            "locale": [ "ru-RU", "en-US" ]
        }
    },
    "network": {
        "downloader": "default"
    },
    "experimentalFeatures": {
        "experimentalMSStore": true,
        "packagedAPI": true
    },
    "telemetry": {
        "disable": true
    }
}
